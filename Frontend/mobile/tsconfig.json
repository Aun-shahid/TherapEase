{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-native",
    "moduleResolution": "bundler", // ‚Üê updated here
    "types": ["react", "react-native"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "app",
    "assets",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
}
